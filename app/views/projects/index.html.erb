<style>
   /* Always set the map height explicitly to define the size of the div
    * element that contains the map. */
   #map {
     height: 100%;
   }
   /* Optional: Makes the sample page fill the window. */
   html, body {
     height: 100%;
     margin: 0;
     padding: 0;
   }
 </style>
</head>
<body>
 <div id="map"></div>
 <script>
   var map;
   function initMap() {
     map = new google.maps.Map(document.getElementById('map'), {
       center: {lat: -9.34, lng: 150.644},
       zoom: 2
     });
   }
 </script>
<script src="https://maps.googleapis.com/maps/api/js?key=ENV['JUST_KEY']&callback=initMap"
 async defer></script>


<h1> 10 Results </h1><br><br>

<% @presenter.projects.each do |project| %>
<table class="table is-bordered is-striped is-fullwidth">
  <thead>
    <th>Name</th>
    <th>Country</th>
    <th>Region</th>
    <th>Approval date</th>
    <th>Closing date</th>
    <th>Total cost</th>
    <th>Team Leader</th>
    <th>Comments</th>
    <th>Follow</th>
  </thead>

  <tbody>
      <tr class="project">
        <td class="name"><%= link_to project.name, project_path(id: project.id)%></td>
        <td class="country"><%= project.country %></td>
        <td class="region"><%= project.region %></td>
        <td class="approval_date"><%= project.approval_date %></td>
        <td class="closing_date"><%= project.closing_date %></td>
        <td class="total_cost"><%= project.total_cost %></td>
        <td class="team_leader"><%= project.team_leader %></td>
        <td></td>
        <td></td>
      </tr>
  </tbody>
</table>
  <table class="table is-bordered is-striped is-fullwidth">
    <tbody>
      <thead>
        <th>Abstract</th>
      </thead>
    <tr>
      <td class="abstract"><%= project.abstract %></td>
    </tr>
  </tbody>
  </table>
<% end %>
